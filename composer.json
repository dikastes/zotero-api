{
    "name": "dikastes/zotero-api",
    "description": "A php wrapper for zotero web api.",
    "keywords": [
        "hedii",
        "zotero-api",
        "zotero",
        "php"
    ],
    "type": "library",
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1",
        "phpunit/phpunit": "^9"
    },
    "homepage": "https://github.com/hedii/zotero-api",
    "support": {
        "issues": "https://github.com/hedii/zotero-api/issues",
        "source": "https://github.com/hedii/zotero-api",
        "email": "hedi.chaibs@gmail.com"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "hedii",
            "email": "hedi.chaibs@gmail.com",
            "homepage": "https://hedichaibi.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hedii\\ZoteroApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hedii\\ZoteroApi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "ci": [ "@phpstan", "@test" ],
        "phpstan": "vendor/bin/phpstan analyse src/ tests/",
        "test": "vendor/bin/phpunit"
    }
}
